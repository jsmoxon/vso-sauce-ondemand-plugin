{
    "manifestVersion": 1,
    "id": "saucelabs-tasks",
    "name": "Sauce Labs integration Tools",
    "version": "0.0.1",
    "publisher": "saucelabs",
    "description": "Tools for building with Sauce labs. Includes three build tasks.",
    "categories": [
        "Build and release"
    ],
    "icons": {
        "default": "saucelabs-icon.png"
    },
    "files": [
        {
            "path": "sauceondemand"
        },
        {
            "path": "sauceondemand-start-sc"
        },
        {
            "path": "sauceondemand-stop-sc"
        }
    ],
    "contributions": [
        {
            "id": "sauceondemand",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": { "name": "Task1" }
        },
        {
            "id": "sauceondemand-start-sc",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": { "name": "Task1" }
        },
        {
            "id": "sauceondemand-stop-sc",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": { "name": "Task2" }
        }
    ]
}
